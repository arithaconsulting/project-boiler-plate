{
  "name": "tsbp-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint:check": "eslint . --ext ts --ext tsx --ext js",
    "eslint:fix": "eslint . --fix",
    "typescript:check": "tsc --pretty --noEmit",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier  --write .",
    "lint-staged": "lint-staged",
    "test-all": "npm run prettier:check && npm run eslint:check && npm run typescript:check && npm run build",
    "project:prepare": "git init && husky install",
    "project:clean": "npm i -g rimraf && rimraf node_modules package-lock.json yarn.lock && npm cache clean --force",
    "project:setup": "npm run project:clean && npm i && npm run project:prepare"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@mui/styled-engine-sc": "^5.11.0",
    "@next/font": "13.1.2",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "eslint-config-next": "13.1.2",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.7.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.1",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "typescript": "^4.9.4"
  }
}
